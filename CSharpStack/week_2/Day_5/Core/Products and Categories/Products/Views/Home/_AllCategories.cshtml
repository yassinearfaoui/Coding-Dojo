
<div style="padding-left: 250px;" class="border-start border-primary">
    <h1>All Categories</h1>
    @{
        foreach(Category category in ViewBag.AllCategories)
        {
               <a asp-action="OneCategorie" asp-controller="Home" asp-route-categoryId="@category.CategoryId"> <h4>@category.Name</h4></a>
        }
    }
</div>
