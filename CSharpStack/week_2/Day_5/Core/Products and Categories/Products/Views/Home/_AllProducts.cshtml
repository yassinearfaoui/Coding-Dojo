
<div style="padding-left: 250px;" class="border-start border-primary">
    <h1>All Products</h1>
    @{
        foreach(Product product in ViewBag.AllProducts)
        {
          <a asp-action="OneProduct" asp-controller="Home" asp-route-productId="@product.ProductId"><h5>@product.Name</h5></a> 
        }
    }
</div>
